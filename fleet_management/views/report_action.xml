<odoo>


    <record id="fleet_income_expense_report_action" model="ir.actions.report">
        <field name="name">Fleet Income &amp; Expense Report</field>
        <field name="model">fleet.vehicle</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">fleet_management.report_fleet_income_expense</field>
        <field name="report_file">fleet_management.report_fleet_income_expense</field>
        <field name="print_report_name">'Fleet Report - %s' % (object.name)</field>
    </record>



    <record id="fleet_maintenance_history_report_action" model="ir.actions.report">
    <field name="name">Fleet Maintenance History Report</field>
    <field name="model">fleet.vehicle</field>
    <field name="report_type">qweb-pdf</field>
    <field name="report_name">fleet_management.report_fleet_maintenance_history</field>
    <field name="report_file">fleet_management.report_fleet_maintenance_history</field>
    <field name="print_report_name">'Fleet Maintenance Report - %s' % (object.name)</field>
    </record>



</odoo>